[32m+[m[32mconst result = sanitizeInput(testInput);[m
[32m+[m[32mconsole.log(result);`,[m
[32m+[m[32m        testCases: [[m
[32m+[m[32m            { input: '<script>console.log("xss")</script>Hello World!', expected: 'Hello World!' },[m
[32m+[m[32m            { input: '<p>Safe text</p>', expected: 'Safe text' },[m
[32m+[m[32m            { input: 'Normal text without tags', expected: 'Normal text without tags' },[m
