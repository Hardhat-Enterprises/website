[32m+[m[32m    has_upper = any(c.isupper() for c in password)[m
[32m+[m[32m    has_lower = any(c.islower() for c in password)[m
[32m+[m[32m    has_digit = any(c.isdigit() for c in password)[m
[32m+[m[32m    has_special = any(c in "!@#$%^&*()_+-=[]{}|;':\",./<>?" for c in password)[m
[32m+[m[41m    [m
[32m+[m[32m    return has_upper and has_lower and has_digit and has_special`[m
[32m+[m[32m    },[m
[32m+[m[32m    javascript: {[m
[32m+[m[32m        title: "JavaScript Security Challenge",[m
[32m+[m[32m        description: "Create a function to sanitize user input by removing potentially dangerous HTML tags and scripts.",[m
[32m+[m[32m        starterCode: `function sanitizeInput(input) {[m
[32m+[m[32m    // Your code here[m
[32m+[m[32m}[m
